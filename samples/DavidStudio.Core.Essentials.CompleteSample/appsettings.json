{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.OpenTelemetry"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "OpenTelemetry",
        "Args": {
          "endpoint": "http://<[host]>:4317",
          "protocol": "Grpc"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "CompleteSampleApi"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CompleteSampleDb": "Server=<server host>;Database=DavidStudio.Services.CompleteSampleDb;User=sa;Password=<sql server password>;TrustServerCertificate=True;"
  },
  "EventBus": {
    "Host": "<ip address>",
    "Port": 5672,
    "VHost": "/",
    "User": "<user name>",
    "Pass": "<password>"
  },
  "JwtAuthorizationOptions": {
    "Issuer": "<issuer url>",
    "Audience": "<audience url>",
    "SecretKey": "<secret key for encrypting and decrypting token at least 128 bits>"
  }
}
